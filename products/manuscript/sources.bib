@article{dudasova2021,
  title = {A Method to Estimate Probability of Disease and Vaccine Efficacy from Clinical Trial Immunogenicity Data},
  author = {Dudášová, Julie and Laube, Regina and Valiathan, Chandni and Wiener, Matthew C. and Gheyas, Ferdous and Fišer, Pavel and Ivanauskaite, Justina and Liu, Frank and Sachs, Jeffrey R.},
  date = {2021-11-04},
